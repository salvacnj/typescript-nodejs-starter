{
  "name": "exegesis-typescript-example",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node --inspect=5858 -r ts-node/register ./src/index.ts",
    "start:watch": "nodemon",
    "setup": "npm install",
    "typedoc": "typedoc --out ./docs --mode modules --tsconfig ./tsconfig.json ./src/",
    "posttypedoc": "shx cp .nojekyll docs/.nojekyll",
    "build": "tsc --build --clean && tsc",
    "build-watch": "tsc --watch",
    "test": "mocha -r ts-node/register src/**/*.spec.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "js-yaml": "^3.14.0",
    "mongoose": "^5.10.0",
    "openapi-mongoose": "^1.0.5",
    "popper.js": "^1.16.1",
    "swagger-ui-express": "^4.1.4",
    "utils-nodejs-scr": "^1.0.27"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "dev"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "devDependencies": {
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.7",
    "@types/mocha": "^8.0.2",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.0.27",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7"
  }
}
