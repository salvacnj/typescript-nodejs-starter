{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "sourceMap": true,
    "esModuleInterop": false,
    "baseUrl": "./",
    "paths": {
      "src/*": [
        "./src/*"
      ],
      "config/*": [
        "./config/*"
      ],
    }
  },
  "include": [
    "src/**/*.ts",
    "src/models/example.ts.example"
  ],
  "exclude": [
    "node_modules",
    ".vscode"
  ]
}
